{
  "version": 2,
  "builds": [
    {
      "src": "preview-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/webhook.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/button-response.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/api/button-response",
      "dest": "/api/button-response.js"
    },
    {
      "src": "/",
      "dest": "/preview-server.js"
    },
    {
      "src": "/api/preview-info",
      "dest": "/preview-server.js"
    },
    {
      "src": "/experiences/(.*)",
      "dest": "/preview-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/preview-server.js"
    }
  ]
}
